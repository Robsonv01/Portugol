programa {
  funcao inicio() {
    real num1, num2, soma, sub, mult, div

    escreva("Informe o primeiro numero: ")
    leia(num1)
    escreva("Informe o primeiro numero: ")
    leia(num2)

    soma = num1 + num2
    sub = num1 - num2
    mult = num1 * num2
    div = num1 / num2
    
    
    escreva("Soma: ", soma)
    escreva("\nSubstração: ", sub)
    escreva("\nMultiplicação: ", mult)
  //  Quebra Linha \n
    escreva("\nDivisão: ", div)


  }
}
